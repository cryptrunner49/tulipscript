// ASCII Examples
struct Cat {}
var kitten = Cat();
kitten.name = "Luna";
kitten.age = 2;
print(kitten.name);  // Outputs: Luna

struct Dog { breed = "Labrador"; age = 3; }
var pup = Dog();
