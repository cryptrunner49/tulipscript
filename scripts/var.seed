// Number
var num = 42;
println(num);  // Outputs: 42

// String
var message = "Hello, Seed!";
println(message);  // Outputs: Hello, Seed!

// Boolean
var isTrue = true;
println(isTrue);  // Outputs: true

// Null
var nothing = null;
println(nothing);  // Outputs: null

// Unicode variable names
var π = 3.14;
println(π);  // Outputs: 3.14

var 挨拶 = "こんにちは";
println(挨拶);  // Outputs: こんにちは

// Emoji variable names
var 🔢 = 100;
println(🔢);  // Outputs: 100

var 💬 = "Emoji!";
println(💬);  // Outputs: Emoji!

var _a = 100;
println(_a);