// Example with while and continue
var x = 0;
print "While loop with continue:";
while (x < 6) {
    x = x + 1;
    if (x == 3) {
        print "Skipping x = 3";
        continue;
    }
    print x;
}
print "Exited while loop";

// Example with for and continue
print "For loop with continue:";
for (var y = 0; y < 6; y = y + 1) {
    if (y == 2) {
        print "Skipping y = 2";
        continue;
    }
    print y;
}
print "Exited for loop";